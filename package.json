{
  "name": "coraza-system-angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run backend:build && concurrently \"npm run backend:start\" \"node server.js\"",
    "start:frontend": "node server.js",
    "build": "npm install && npm install -g @angular/cli && ng build --configuration production && npm run backend:build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "backend:build": "tsc -p src/services/tsconfig.json",
    "backend:start": "node ./src/services/dist/server.js",
    "backend:dev": "nodemon --watch src/services -e ts --exec ts-node -P src/services/tsconfig.json src/services/server.ts"
  },
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/cli": "^16.2.16",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "pg": "^8.11.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/compiler-cli": "^16.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^24.2.1",
    "@types/pg": "^8.15.5",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "typescript": "~5.1.3"
  }
}
