{
  "name": "coraza-system-angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "npm run build && npm run backend:build && node ./src/services/dist/server.js",
    "dev": "ng serve --proxy-config proxy.conf.json",
    "build": "npx --yes @angular/cli@16.2.16 build --configuration production",
    "build:prod": "ng build --configuration production",
    "build:dev": "npx --yes @angular/cli@16.2.16 build",
    "watch": "npx --yes @angular/cli@16.2.16 build --watch --configuration development",
    "test": "npx --yes @angular/cli@16.2.16 test",
    "init-db": "node init-database.js",
    "migrate": "node migrate-database.js",
    "backend:build": "tsc -p src/services/tsconfig.json",
    "backend:start": "node ./src/services/dist/server.js",
    "backend:dev": "nodemon --watch src/services -e ts --exec ts-node -P src/services/tsconfig.json src/services/server.ts"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/cli": "^16.2.16",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@supabase/supabase-js": "^2.57.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "^5.1.6",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jasmine": "~4.3.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^18.15.13",
    "@types/pg": "^8.15.5",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
